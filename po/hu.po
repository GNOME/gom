msgid "SQL"
msgstr "SQL"

#: ../gom/gom-filter.c:507
msgid "The sql for the filter."
msgstr "Az SQL a szűrőhöz."

#: ../gom/gom-cursor.c:299
msgid "Statement"
msgstr "Utasítás"

